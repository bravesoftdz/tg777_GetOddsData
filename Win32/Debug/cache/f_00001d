/*千分號*/
var thousandComma = function(number)
{
 var num = number.toString();
 var pattern = /(-?\d+)(\d{3})/;
  
 while(pattern.test(num))
 {
  num = num.replace(pattern, "$1,$2");
  
 }
 return num;
 
}
/*檢查game是否關閉或日期已過*/
function get_game_or_stop(a)
{
	//var ss = parent.window.frames.jQuery("#movemore_ss").load("./controler/stopbet.php",{"gameid" : a});
  var result;
	
	jQuery.ajax({
			type: 'POST',
			url: './stopbet.php',
			data: 'gameid=' + a,	
			error: function(xhr) {
				//alert('網路忙線中，請稍後再試!r64');
				result = false;
			},
			async: false,
			//beforeSend:function(response)
		  //{		  
		  //},
			success: function(response) {
				//if(response=='1')
				 //{
		   //parent.window.top.location.href='../list.php';
		     result = response;
				 //}
				 
			 }
	});
	
  return result;
  
}

function reloadtoploginform(){
   
   jQuery.ajax({
			type: 'POST',
			url: './controler/login.php',
			data: 'reload=reload',
			error: function(xhr) {
				//alert('網路忙線中，請稍後再試!session95');
			},
			beforeSend:function(response)
		  {
		  	jQuery('#wa99_login').html('..loading....');
		  	//jQuery('#openallsel2' + c +'').css({'height':'1000px'});
		  	//jQuery('#openallsel2' + c +'').html('<font size="6px">..loading....</font>');
		  },
			success: function(response) {
			 jQuery('#wa99_login').html(response);
			 }
		   });
		  
}

