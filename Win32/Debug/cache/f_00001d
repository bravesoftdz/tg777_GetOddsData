function aboutrule(a)
{
/*无下注注单!*/
var s = jQuery('#order').html();
if(s=="") jQuery('#order').html('<table><tr><td></td></tr></table><table style="width:100%;"><tr><td style=height:10px;text-align:center;></td></tr><tr><td style=height:20px;padding-left:10px;>无下注注单!</td></tr><tr><td style=height:10px;text-align:center;></td></tr></table>');
	
	if(a=="1")
	{

jQuery('#order').attr("disabled", false);
jQuery('#order').show();
jQuery('#rule').hide();
jQuery('#order_info a').css({'color':'#FFFFFF'});
jQuery('#rule_anu a').css({'color':'#000000'});
jQuery('#rule_anu').css({'background-image':'url(./images/666/orderinfo_w.png)'});
jQuery('#order_info').css({'background-image':'url(./images/666/orderinfo.png)'});
	}
	else if(a=="2")
	{
		
	}
	else
	{
	
	jQuery('#order').hide();
	jQuery('#rule').show();
	jQuery('#rule1').show();
	jQuery('#rule_anu a').css({'color':'#FFFFFF'});
	jQuery('#order_info a').css({'color':'#000000'});
	jQuery('#order_info').css({'background-image':'url(./images/666/orderinfo_w.png)'});
	jQuery('#rule_anu').css({'background-image':'url(./images/666/orderinfo.png)'});
	}
}

function checkquota_orderfinish()
{
	//alert('Y');
	jQuery.ajax({
		type: 'GET',
		url: './index3.php?option=com_quota&Itemid=101',
		data: 'check=chkquota',
		error: function(xhr) {
			//alert('error');
			return false;
		},
		beforeSend:function(response)
		  {
		  	//jQuery('#checkquota').html('...網路忙線中...');
		  	//return false;
		  },
		success: function(response) {
		//alert(response);
		jQuery('#checkquota', window.parent.document).html(response);
		}
	});
}

function addnumber(obj)
{
	 if(obj < 10 ) return "0" +""+ obj;
   else return obj;

}