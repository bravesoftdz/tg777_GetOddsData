    totals=totals+'<input type="hidden" name="handicap" id="handicap" value="'+a7+'">';
    totals=totals+'<input type="hidden" name="gamename" id="gamename" value="'+decodeURI(a)+'">';
    totals=totals+'<input type="hidden" name="marketname_st" id="marketname_st" value="'+decodeURI(d)+'">';
    totals=totals+'<input type="hidden" name="Rate" id="Rate" value="'+f+'">';
    totals=totals+'<input type="hidden" name="percent" id="percent" value="'+k+'">';
    totals=totals+'<input type="hidden" name="betfairori" id="betfairori" value="'+j+'">';
    totals=totals+'<input type="hidden" name="markettype" id="markettype" value="'+decodeURI(a3)+'">';
    totals=totals+'<input type="hidden" name="time" id="time" value="'+decodeURI(a4)+'">';
    totals=totals+'<input type="hidden" name="selectname" id="selectname" value="'+decodeURI(a5)+'">';
    totals=totals+'<input type="hidden" name="inplay" id="inplay" value="'+a6+'">';
	  
	  //jQuery('#movemore_ss', window.parent.document).html(totals);
	  //jQuery('#movemore_ss', window.parent.document).html(totals);
	    //parent.window.frames.leftmenu.jQuery('#order').html(totals);
	    jQuery('#order').html(totals);
	  //jQuery("#flotLayer").html(totals);
