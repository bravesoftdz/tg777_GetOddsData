function checktime5_5(a3,a2,a1,a,d,f,h)
  {
   
   
   time_s_sec = time_s_sec - 1;
	 
			  if(time_s_sec < 0)
			  	 {
        openallselwill_sec(a3,a2,a1,a,d,f,h);
        clearInterval(chkstop_open);
        window.clearInterval(chkstop_open);
        time_s_sec=60;
        //return;
			  	 }
			  else
			  	 {
			//jQuery('#muorder_sec').html('等待時間'+time_s_sec+'秒');  	
			  	 }
			 
      if(time_s_sec < 10)
      {
   parent.window.frames.rightmenu.jQuery('#reloadpic_pic').html('<div style="font-size:14px;position:relative;height:30px;top:5px;">更新<br>0'+time_s_sec+'</div>');
      }
      else
      {
   parent.window.frames.rightmenu.jQuery('#reloadpic_pic').html('<div style="font-size:14px;position:relative;height:30px;top:5px;">更新<br>'+time_s_sec+'</div>');
      }
  chkstop_open=setTimeout("checktime5_5('"+a3+"','"+a2+"','"+a1+"','"+a+"','"+d+"','"+f+"','"+h+"')", 1000);
  }
 
  
function checktime5(a3,a2,a1,a,d,f,h)
  {
   parent.window.frames.rightmenu.jQuery('#reloadpic_pic').css({"opacity":"0.5","filter":"Alpha(Opacity=50, Style=0)"});
	 parent.window.frames.rightmenu.jQuery('#reloadpic_pic').attr('onclick','');
   parent.window.frames.rightmenu.jQuery('#reloadbutton').attr('disabled','disabled');
   
   if(chkstop_open)
  	 {
   clearInterval(chkstop_open);
   time_s_sec = 60;
   checktime5_5(a3,a2,a1,a,d,f,h);
  	 }
   openallselwill_sec(a3,a2,a1,a,d,f,h);
  }
  

