var count_night;
function countdown_ceo2()
{
	countdowntime -= 1;
	var img = "<img src=./components/com_main/images/28.gif>";
	 if(countdowntime <= 0)
	 	{
	 	return false;
	 	}
	 else
	 	{
	 jQuery('#order').html("<font color=#FF0000><br><br>滚球倒数" + countdowntime + "秒</font>");
	 	}
  count_night = setTimeout("countdown_ceo2()", 1000);
}

function closeorder(a)
{
	if(a=='889')
	{
	  document.location.href="./controler/memberlogout.php"
		return;
	}
	//alert('b');
	//jQuery(this).dialog("close");
	//alert('a_dd');
	window.clearInterval(a_add);
  a_add=0;
  
	 //parent.window.frames.jQuery("#movemore_ss").html('');
	 //parent.window.frames.jQuery("#movemore_ss").dialog("close");
	 //parent.window.frames.jQuery("#movemore_ss").html('');
	 //jQuery('#movemore_ss', window.parent.document).html('');
	 jQuery('#movemore_ss', window.parent.document).dialog("close");
	 //jQuery('#movemore_ss', window.parent.document).html("");
  //jQuery('#order').attr("disabled", false);
  //jQuery('#order').html('');
  //parent.window.frames.jQuery('#movemore_ss').attr("disabled", true);
  //parent.window.frames.jQuery('#movemore_ss').html('ARTT');
   //jQuery('#movemore_ss').dialog("close");
   //window.document.getElementById("movemore_ss").style.visibility="hidden";
   
     //window.clearInterval(a_add);
 
   
}

function orderinfo_link()
{
	window.location.href='./transaction.php';
}

function closeorder_sec()
{
	document.getElementById("movemore_ss").style.visibility="hidden";
	jQuery('#movemore_ss', window.parent.document).attr("disabled", false);
}

function openleft(a)
{
	var ss = jQuery('#left1_10').css('display');

	for(i=0; i < a; i++)
		{	
			jQuery('#lefttitle'+i+'').toggle();
			for(j=0; j < 30; j++)
			  {
			  	jQuery('#left1_'+i+j+'').toggle();
			    jQuery('#left2_'+i+j+'').toggle();
			  }
		}
	if(ss=="none")
	 {
	jQuery('#openclose').html('open');
   }
   else
   	{
  jQuery('#openclose').html('close'); 		
   	}
}


