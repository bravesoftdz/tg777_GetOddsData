	 jQuery.ajax({
		type: 'GET',
		url: './index2.php?option=com_newopen&Itemid=101',
		//url: './index2.php?option=com_rightsimpless&Itemid=101',
		//url: './components/com_rightsimpless/php/simplereload.php',
