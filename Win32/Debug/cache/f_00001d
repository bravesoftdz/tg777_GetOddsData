function getDateRange(rangetime,a) 
{
/*
var month_range=new Array("January","February","March","April","May","June","July","August","September","October","November","December");
*/
var monthr=new Array(12);
    monthr[01]="January";
    monthr[02]="February";
    monthr[03]="March";
    monthr[04]="April";
    monthr[05]="May";
    monthr[06]="June";
    monthr[07]="July";
    monthr[08]="August";
    monthr[09]="September";
    monthr[10]="October";
    monthr[11]="November";
    monthr[12]="December";

    var res = rangetime.split("+");
    var res_left = res[0].split("-");
    var res_endrange = res[1].split(":");
    var d=new Date();
    //var f = res[0]+'T'+res[1];
    
    var f = monthr[parseInt(res_left[1])]+" "+res_left[2]+", "+res_left[0]+" "+res[1];
    //alert(monthr[parseInt(res_left[1])]);
    var endrange = res[0]+" "+res_endrange[0]+":"+res_endrange[1];
    //alert(monthr[res_left[1]]);
    //var ggyy = new Date(f);
    //var ggyy = new Date("2014-12-20T03:30");
    //var ggyy = new Date("December 30, 2014 17:13:00");
    var ggyy = new Date(f);
    //var ss = (ggyy.getTime() + 1000*60*60*3); //加3小時
    var ss = ggyy.getTime();
    //alert(ggyy);
    /*
    var range1 = new Date(ss);
    var stringrange = range1.getFullYear() + "-" +
                     (range1.getMonth() + 1)+ "-" +
                     range1.getDate() + " " +
                     gettow((range1.getHours() + 3)) + ":" +
                     gettow(range1.getUTCMinutes());
    */
    
    var localTime = d.getTime();
    var localOffset=d.getTimezoneOffset()*60000; //°¾²¾ªº²@¬뽊    
    var utc = localTime + localOffset; //utc§YGMT
    var offset =8; //¥xŗ¥[8
    var Taiwan = utc + (3600000*offset); 
    //var theDates = new Date(Taiwan);
    var theDates = new Date(d.getFullYear(),d.getMonth(),d.getDate(),d.getHours(),'00');
    
    var rangess = [];
    while(theDates.getTime() < ss) {
    	
        var string = theDates.getFullYear() + "-" +
                     (theDates.getMonth() + 1)+ "-" +
                     theDates.getDate() + " " +
                     gettow(theDates.getHours()) + ":" +
                     gettow(theDates.getUTCMinutes());
        rangess.push(string);
        //theDate.setTime(theDate.getTime() + 86400000);//1 hours     
        theDates.setTime(theDates.getTime() + 1000*60*15);//30分鐘 
        
        }


var sst='<select name=f_date'+a+' id=f_date'+a+'>';
for(i=0; i < rangess.length; i++)
{
//document.writeln(getDateRange()[i] + "<br>");
sst=sst+'<option value="'+rangess[i]+'">'+rangess[i]+'</option>';
}
sst=sst+'<option value="'+endrange+'">'+endrange+'</option>';
sst=sst+'</select>';
return sst;

}

function gettow(a)
{
	if(a < 10)
	 {
	 	 return '0'+a;
	 }
	else
	 {
	 	 return a;
	 }
}

function appointorder(a7,a6,a55,a4,a3,a2,an5,b,c,d,f,g,h,j,k)
