  	simple_ar.push(jimDatass);
  	orderinfo_st = simple_ar;
  	c2bet_st["c2betorder"]=orderinfo_st;
 	
   //jQuery('#movemore_ss', window.parent.document).html(total);
  //parent.window.frames.leftmenu.jQuery('#order').html(total);
  jQuery('#order').html(total);

