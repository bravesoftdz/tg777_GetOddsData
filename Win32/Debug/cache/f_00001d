		  	},
		success: function(response) {
			//alert(response);
			 //jQuery('#rightAAA', window.parent.document).html('');
			 var su_md = new Date().getTime();
			var suc = parseInt(su_md / 100);
			var sec = parseInt(left_md / 100);
			 jQuery('#order_top').attr("style", "display:block;");
			 jQuery('#openallsel2').html(response+'<font color=#ffffff>'+(suc - sec)+'</font>');
       //parent.window.frames.rightmenu.jQuery('#openallsel2').css({'ckground-image':'url(./images/666/content-wrapper_bg.jpg)'});
		}
	
	 });
	 
}
/*釋放reload*/
function clearen()
{
	            clearInterval(chkstop);
            	window.clearInterval(chkstop);
            	clearInterval(chkstop_now);
            	window.clearInterval(chkstop_now);
            	/*chkstop = '';*/
            	
}


var a_add=0;
function checkorder_add(bb,b)
  {
    if(b=='all')
     {
  var bet = '0';   	
     }
    else
    	{
  var bet = jQuery('#Bet').val();  		
    	}
  	var d = jQuery('#BetType').val();
  	var c = jQuery('#Rate').val();
  	var f = jQuery('#percent').val();  	
  	
  	var balance_money = parent.window.frames.jQuery('#balance_money_chk').val();
  	
  	if(bet=='')
  	{
  		bet=0;
  	}
  	if(c=="")
  	{
  		//alert("赔率空值!");
  		swal({title: "错误讯息",text: "赔率空值!",confirmButtonText: "确定",customClass: 'twitter'});
  		return;
  	}
  	var total_ss = ((a_add-0)+(bet-0)+(bb-0));
  	
  	if((total_ss+0) > (balance_money+0))
  	{
  		swal({title: "错误讯息",text: "大于可用馀额!",confirmButtonText: "确定",customClass: 'twitter'});
  		return;
  	}
  	
  	if(d=="B")
  	 {
  	var ss = ((a)*(1-f)*(c-1)).toFixed(2);
    jQuery('#ordertotal').html(ss);
    jQuery('#orderrisk').html(a);
  	 }
  	 else
  	 	{
   var ss = ((total_ss)*(c)*(1-f)).toFixed(2);
   var hh = ((total_ss)*(1-f)).toFixed(2);
   jQuery('#ordertotal').html(hh);
   jQuery('#orderrisk').html(thousandComma(ss));
   
   jQuery('#Bet').val(parseInt(total_ss));
  	 	}
  	
  }
  
function checkorder(bb)
  {
    window.clearInterval(a_add);
    clearInterval(a_add);
    a_add=0;
  	var d = jQuery('#BetType').val();
  	var c = jQuery('#Rate').val();
  	var f = jQuery('#percent').val();
  	
  	
  	//var reg = /^[0-9]+(\.)?$/;
  	var reg = /^[0-9]*$/;
  	//var reg = /^[0-9]+(\.[0-9]{0,0})?$/;
  	var sre = /^[0-9]+(\.[0-9]{1,2})?$/;//小數點2位
  	if(c=="")
  	{
  		swal({title: "错误讯息",text: "赔率空值!",confirmButtonText: "确定",customClass: 'twitter'});
  		return;
  	}
  	if(!sre.test(bb))
	    {
	    	//alert('金额请勿有小数点或是其他文字');
	    	swal({title: "错误讯息",text: "金额最多小数点2位!",confirmButtonText: "确定",customClass: 'twitter'});
	    	return;
	    }
	    
	  
  	if(d=="B")
  	 {
  	var ss = ((bb)*(1-f)*(c-1)).toFixed(2);
    jQuery('#ordertotal').html(ss);
    jQuery('#orderrisk').html(a);
  	 }
  	 else
  	 	{
   //alert('T');
   var ss = ((bb)*(c)*(1-f)).toFixed(2);
   var hh = ((bb)*(1-f)).toFixed(2);
   jQuery('#ordertotal').html(hh);
   jQuery('#orderrisk').html(thousandComma(ss));

  	 	}
  
  }

function checkorder_appoint(a)
  {
  	//var ss = ((a*(b-1))*(1-c)).toFixed(2);
  	//var ss = ((a*b)*(1-c)).toFixed(3);
  	re = /^[0-9]+(\.)?$/;
  	var ee = /^\d+$/;
  	var d = jQuery('#BetType').val();
  	var c = jQuery('#Bet').val();
  	var f = jQuery('#percent').val();
  	
  	if (!ee.test(c))
    {
        //非正整數
        alert("请输入正确注额");
        return;
    }
  	if(!re.test(c))   
       {    
     alert("注额请勿有小数点或是其他文字");
     return;
       }
        
  
  	
  	if(d=="B")
  	 {
  	var ss = ((c)*(1-f)*(a-1)).toFixed(2);
    jQuery('#ordertotal').html(ss);
    jQuery('#orderrisk').html(c);
  	 }
  	 else
  	 	{
   var ss = ((c)*(a-1)).toFixed(2);
   var hh = ((c)*(1-f)).toFixed(2);
   jQuery('#ordertotal').html(hh);
   jQuery('#orderrisk').html(ss);
  	 	}
  	
  }

function checkorder_appoint_max(a)
  {
  	//var ss = ((a*(b-1))*(1-c)).toFixed(2);
  	//var ss = ((a*b)*(1-c)).toFixed(3);
  	var d = jQuery('#BetType').val();
  	var c = jQuery('#Bet').val();
  	var f = jQuery('#percent').val();
  	
  	
  	if(d=="B")
  	 {
  	var ss = ((c)*(1-f)*(a-1)).toFixed(2);
    jQuery('#ordertotal_2').html(ss);
    jQuery('#orderrisk_2').html(c);
  	 }
  	 else
  	 	{
   var ss = ((c)*(a-1)).toFixed(2);
   var hh = ((c)*(1-f)).toFixed(2);
   jQuery('#ordertotal_2').html(hh);
   jQuery('#orderrisk_2').html(ss);
  	 	}
  	
  } 
  
function getorder()
  {
    //alert('hh');
  	var simple_ar = Array();
  	var c2bet_st = new Object;
  	var orderinfo_st = new Object;
  	var ee = /^\d+$/;
  	//var re = /^[0-9]+(\.)?$/;
  	var re = /^[0-9]*$/;
  	var MarketId = jQuery('#MarketId').val();
  	var gameid = jQuery('#gameid').val();
  	var SelectionId = jQuery('#SelectionId').val();
  	var BetType = jQuery('#BetType').val();
  	var name_st = jQuery('#name').val();
  	var gamename = jQuery('#gamename').val();
    var marketname = jQuery('#marketname_st').val();
    var Rate = jQuery('#Rate').val();
    var momey = jQuery('#momey').val();
    var percent = jQuery('#percent').val();
    var betfairori = jQuery('#betfairori').val();
    var Bet = jQuery('#Bet').val();
    var markettype = jQuery('#markettype').val();
    var time = jQuery('#time').val();
    var selectname = jQuery('#selectname').val();
    var inplay = jQuery('#inplay').val();
    var handicap = jQuery('#handicap').val();
    var selectrateL1 = jQuery('#selectrateL1').val();
    var gc12 = jQuery('#gc12').val();
    var mid = jQuery('#mid').val();
    var who = jQuery('#who').val();
    var mname = jQuery('#mname').val();
    var category = jQuery('#category').val();
    var sel = jQuery('#sel').val();
    var balance_money = parent.window.frames.jQuery('#balance_money_chk').val();
  	var sre = /^[0-9]+(\.[0-9]{1,2})?$/;//小數點2位	
  		
    var ssB = ((Bet)*(1-percent)*(Rate-1)).toFixed(2);   
    //var ss = ((Bet)*(Rate-1)).toFixed(2);
    //var ss = ((Bet)*(Rate/100)).toFixed(2);
    var ss = ((Bet)*(Rate)*(1-percent)).toFixed(2);
    var hh = ((Bet)*(1-percent)).toFixed(2);
    
    var kk = percent*100;   
	  var commision = '<span style=font-size:14px;padding-right:10px;float:right;padding-top:2px;>手续费：'+kk+'%</span>';
    
    //alert(name_st);
    //return;
    //var total='***投注資訊***\n[賽事]'+a+'\n[類別]'+b+'\n[選項]'+c+'\n\n***注單資訊***\n[投注注額]'+Betorder+'\n[投注賠率]'+risk_st+'\n\n你確定下注?';
