function getorder_appoint()
  {
    //alert(hh);
  	var simple_ar = Array();
  	var c2bet_st = new Object;
  	var orderinfo_st = new Object;
  	var ee = /^\d+$/;
  	re = /^[0-9]+(\.)?$/;
  	var MarketId = jQuery('#MarketId').val();
  	var gameid = jQuery('#gameid').val();
  	var SelectionId = jQuery('#SelectionId').val();
  	var BetType = jQuery('#BetType').val();
  	var name_st = jQuery('#name').val();
  	var gamename = jQuery('#gamename').val();
    var marketname = jQuery('#marketname_st').val();
    var Rate = jQuery('#Rate').val();
    var momey = jQuery('#momey').val();
    var percent = jQuery('#percent').val();
    var betfairori = jQuery('#betfairori').val();
    var Bet = jQuery('#Bet').val();
    var markettype = jQuery('#markettype').val();
    var time = jQuery('#time').val();
    var selectname = jQuery('#selectname').val();
    var inplay = jQuery('#inplay').val();    
    var handicap = jQuery('#handicap').val();
    
    var f_date1 = jQuery('#f_date1').val();
    var f_date2 = jQuery('#f_date2').val();
    
    var range1 = jQuery('#range1').val();
    var range2 = jQuery('#range2').val();
    
    var gc12 = jQuery('#gc12').val();
    var mid = jQuery('#mid').val();
    var who = jQuery('#who').val();
    var mname = jQuery('#mname').val();
    
    var ssB = ((Bet)*(1-percent)*(range1-1)).toFixed(2); 
    var ssB_2 = ((Bet)*(1-percent)*(range2-1)).toFixed(2);   
    
    var ss = ((Bet)*(range1-1)).toFixed(2);
    var ss_2 = ((Bet)*(range2-1)).toFixed(2);
    var hh = ((Bet)*(1-percent)).toFixed(2);
    
    
    var total='<table border="0" align="left" cellpadding="0" cellspacing="0" class="secwin">';
	  if(BetType=="B")
	  {
	  total=total+'<tr class=trmid_B><td colspan=4 height="40" align="left" valign="middle" style="padding-left:15px;">类别:&nbsp;'+gamename+'</strong></td></tr>';
	  total=total+'<tr class=trmid_B><td colspan=4 height="40" align="left" valign="middle" style="padding-left:15px;">选项:&nbsp;<strong>'+marketname+'/'+selectname+'&nbsp;<font color=#0063C6>(闲)</font></strong></td></tr>';
	  total=total+'<tr class=trmid_B_B_bottom_se><td colspan=4 height="40" align="left" valign="middle" style="padding-left:15px;">投注注额:&nbsp;<strong>'+Bet+'</strong></td></tr>';
	  total=total+'<tr class=trmid_B_B_bottom_se><td colspan=4 height="40" align="left" valign="middle" style="padding-left:15px;">投注赔率区间:&nbsp;<strong>'+range1+'</strong>&nbsp;--&nbsp;<strong>'+range2+'</strong><td></tr>';
	  total=total+'<tr class=trmid_B_B_bottom_se><td colspan=4 height="40" align="left" valign="middle" style="padding-left:15px;">可赢点数区间:&nbsp;<strong>'+ssB+'&nbsp;--&nbsp;'+ssB_2+'</strong></td></tr>';
	  total=total+'<tr class=trmid_B_B_bottom_se><td colspan=4 height="40" align="left" valign="middle" style="padding-left:15px;">风险值區間:&nbsp;<strong>'+Bet+'&nbsp;--&nbsp;'+Bet+'</strong></td></tr>';	  
	  //total=total+'<tr class=trmid_RR_bottom_color_b><td colspan=4 height="80" align="left" valign="middle" style="padding-left:15px;">您在买'+marketname+'/'+selectname+'<br>若您赢了,您将赢得<font color=#00A200>' + ssB + '</font>金额<br>若您输了,则失去<font color=#FF0000>'+Bet+'</font>金额</td></tr>';	       	
	  total=total+'<tr class=trmid_B_B_bottom_se><td colspan=4 height="40" align="left" valign="middle" style="padding-left:15px;">时间区间:&nbsp;<strong>'+f_date1+'</strong>&nbsp;--&nbsp;<strong>'+f_date2+'</strong></td></tr>';
	  total=total+'<tr class=trmid_RR_bottom_color_b><td colspan=4 height="80" align="left" valign="middle" style="padding-left:15px;">您在买'+marketname+'/'+selectname+'</td></tr>';	       	
	  
	  }
	  else
	  {
	  total=total+'<tr class=trmid_B><td colspan=4 height="40" align="left" valign="middle" style="padding-left:15px;">类别:&nbsp;'+gamename+'</strong></td></tr>';
	  total=total+'<tr class=trmid_B><td colspan=4 height="40" align="left" valign="middle" style="padding-left:15px;">选项:&nbsp;<strong>'+marketname+'/'+selectname+'&nbsp;<font color=#FF0000>(庄)</font></strong></td></tr>';
	  total=total+'<tr class=trmid_B_B_bottom_se><td colspan=4 height="40" align="left" valign="middle" style="padding-left:15px;">投注注额:&nbsp;<strong>'+Bet+'</strong></td></tr>';
	  total=total+'<tr class=trmid_B_B_bottom_se><td colspan=4 height="40" align="left" valign="middle" style="padding-left:15px;">投注赔率区间:&nbsp;<strong>'+range1+'</strong>&nbsp;--&nbsp;<strong>'+range2+'</strong><td></tr>';
	  total=total+'<tr class=trmid_B_B_bottom_se><td colspan=4 height="40" align="left" valign="middle" style="padding-left:15px;">可赢点数区间:&nbsp;<strong>'+hh+'&nbsp;--&nbsp;'+hh+'</strong></td></tr>';
	  total=total+'<tr class=trmid_B_B_bottom_se><td colspan=4 height="40" align="left" valign="middle" style="padding-left:15px;">风险值區間:&nbsp;<strong>'+ss+'&nbsp;--&nbsp;'+ss_2+'</strong></td></tr>';
	  
	  //total=total+'<tr class=trmid_RR_bottom_color_r><td colspan=4 height="80" align="left" valign="middle" style="padding-left:15px;">您在反对'+marketname+'/'+selectname+'会赢<br>若您赢了,您将赢得<font color=#00A200>'+hh+'</font>金额<br>若您输了,则失去<font color=#FF0000>'+ss+'</font>金额</td></tr>';	
    total=total+'<tr class=trmid_B_B_bottom_se><td colspan=4 height="40" align="left" valign="middle" style="padding-left:15px;">时间区间:&nbsp;<strong>'+f_date1+'</strong>&nbsp;--&nbsp;<strong>'+f_date2+'</strong></td></tr>';
	  total=total+'<tr class=trmid_RR_bottom_color_r><td colspan=4 height="80" align="left" valign="middle" style="padding-left:15px;">您在反对'+marketname+'/'+selectname+'会赢</td></tr>';	
       		
	  }
	  //total=total+'<tr class=new-order_m_m><td><div id=btnTest>確定交易</div></td><td><a href=javascript:void(0) onclick="closeorder()">取消交易</a></td></tr></table>';
	  total=total+'<tr class=new-order_m_m><td colspan=2><input type=button id=btnTest value=确定交易></td><td colspan=2><input type=button value=取消交易 onclick="closeorder()"></td></tr>';
	
	if (!ee.test(Bet))
    {
        //非正整數
        alert("请输入正确注额");
        return;
    }  
	
	if(!re.test(Bet))   
       {    
     alert("注额请勿有小数点或是其他文字");
     return;
       }
            
 
 if(Bet < 1 || Bet=="")
  	{
  		alert('注额请大于1');
  		return;
  	}

 
 /*
 if(!confirm(total))
    {
    	return false;
    
    }	
 */

